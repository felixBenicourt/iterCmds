            "content": {
                "widget": [
                    {
                        "type": "text_editor",
                        "label": "Python Code Editor",
                        "key": "python_code",
                        "default": "\n\nimport maya.cmds as cmds\n\n\ndef print_connected_node(joint, rotation_attr):\n    joint_attr = \"{}.{}\".format(joint, rotation_attr)\n\n    existing_connections = cmds.listConnections(joint_attr, plugs=True, destination=True)\n    \n    if not existing_connections:\n        print(\"No connection found for {}.\".format(joint_attr))\n        return None\n    else:\n        connected_attr = existing_connections[0]\n        connected_node = connected_attr.split('.')[0]\n        print(\"Node connected to {}: {}\".format(joint_attr, connected_node))\n        return connected_attr\n\n\ndef insert_set_range_between(joint, rotation_attr, old_min, old_max, new_min, new_max):\n    connected_attr = print_connected_node(joint, rotation_attr)\n    \n    if not connected_attr:\n        return\n\n    connected_node = connected_attr.split('.')[0]\n    target_attr = \"{}.{}\".format(joint, rotation_attr)\n\n    if cmds.nodeType(connected_node) == 'unitConversion':\n        unit_conversion_input = cmds.listConnections(\"{}.input\".format(connected_node), plugs=True, source=True)\n        \n        if unit_conversion_input and cmds.nodeType(unit_conversion_input[0].split('.')[0]) == 'setRange':\n            set_range_node = unit_conversion_input[0].split('.')[0]\n            print(\"Found existing setRange node {} connected to {}.\".format(set_range_node, connected_node))\n        else:\n            set_range_node = cmds.createNode('setRange', name='setRangeNode')\n            cmds.disconnectAttr(connected_attr, \"{}.input\".format(connected_node))\n            cmds.connectAttr(set_range_node + \".outValueX\", \"{}.input\".format(connected_node))\n            cmds.connectAttr(connected_attr, \"{}.valueX\".format(set_range_node))\n            print(\"Created new setRange node {} and connected to {}.\".format(set_range_node, connected_node))\n    else:\n        set_range_node = cmds.createNode('setRange', name='setRangeNode')\n        cmds.disconnectAttr(connected_attr, target_attr)\n        cmds.connectAttr(connected_attr, \"{}.valueX\".format(set_range_node))\n        cmds.connectAttr(\"{}.outValueX\".format(set_range_node), target_attr)\n        print(\"Created new setRange node {}.\".format(set_range_node))\n\n    axis = rotation_attr[-1].upper()\n\n    cmds.setAttr(\"{}.oldMin{}\".format(set_range_node, axis), old_min)\n    cmds.setAttr(\"{}.oldMax{}\".format(set_range_node, axis), old_max)\n    cmds.setAttr(\"{}.min{}\".format(set_range_node, axis), new_min)\n    cmds.setAttr(\"{}.max{}\".format(set_range_node, axis), new_max)\n    \n    print(\"Updated setRange node {} with min{}={}, max{}={}, oldMin{}={}, oldMax{}={}.\".format(\n        set_range_node, axis, new_min, axis, new_max, axis, old_min, axis, old_max))\n\n\ninsert_set_range_between(\n    variables[\"joint\"], \n    variables[\"attribut\"], \n    float(variables[\"initMin\"]), \n    float(variables[\"initMax\"]), \n    float(variables[\"newMin\"]), \n    float(variables[\"newMax\"])\n)\n"
                    }
